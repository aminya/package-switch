{
  "name": "package-switch",
  "main": "lib/main.js",
  "version": "0.8.0",
  "author": "Amin Yahyaabadi, Fabian Stiewitz",
  "description": "Easy package activation/deactivation",
  "keywords": [
    "package",
    "activate",
    "deactivate",
    "activation",
    "disable",
    "enable",
    "load",
    "bundle",
    "switch",
    "shutdown"
  ],
  "repository": "https://github.com/aminya/package-switch",
  "license": "MIT",
  "scripts": {
    "typescript": "tsc -p ./src/tsconfig.json || echo done",
    "prettier_src": "prettier --write ./src/**/*.ts",
    "prettier_out": "prettier --write ./lib/**/*.js",
    "buildlocal": "npm run typescript -s",
    "lint": "eslint . --ext js,ts,coffee,json",
    "test": "npm run buildlocal && atom --test spec",
    "prepare": "npm run build",
    "ppublish": "bash ./ppublish.sh",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "space-pen": "^5.1.2",
    "jquery": "2.1.4",
    "season": "^6.0.2"
  },
  "devDependencies": {
    "typescript": "^3.8.3",
    "coffeescript": "^2.5.1",
    "@types/atom": "^1.40.1",
    "@types/season": "^6.0.0",
    "eslint": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-eslint": "latest",
    "eslint-plugin-coffee": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-only-warn": "latest",
    "tslint": "latest",
    "tslint-config-prettier": "latest",
    "prettier": "latest",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "rollup-plugin-coffee-script": "^2.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "rollup": "^2.7.3"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 120
  }
}
